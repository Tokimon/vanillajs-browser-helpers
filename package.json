{
  "name": "vanillajs-browser-helpers",
  "version": "1.1.4",
  "description": "Collection of convenience code snippets (helpers) that aims to make it a little easier to work with vanilla JS in the browser",
  "author": "Toke Voltelen",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "karma start",
    "build": "webpack --config webpack.es5.config.js",
    "build:spec": "webpack --config webpack.spec.config.js",
    "jsdoc": "node ./tasks/jsdoc.js",
    "coveralls": "cat ./coverage/lcov/lcov.info | node node_modules/.bin/coveralls",
    "coveralls:win": "type ./coverage/lcov/lcov.info | node node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Tokimon@github.com/Tokimon/vanillajs-browser-helpers.git"
  },
  "keywords": [
    "javascript",
    "helpers",
    "find",
    "event",
    "scroll",
    "css"
  ],
  "bugs": {
    "url": "https://github.com/Tokimon/vanillajs-browser-helpers/issues"
  },
  "homepage": "https://github.com/Tokimon/vanillajs-browser-helpers#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "chai-arrays": "^2.0.0",
    "chai-as-promised": "^7.1.1",
    "chai-dom": "^1.8.1",
    "chai-things": "^0.2.0",
    "coveralls": "^3.0.2",
    "fs-promise": "^2.0.3",
    "globby": "^9.1.0",
    "karma": "^4.0.1",
    "karma-chai": "^0.1.0",
    "karma-chai-dom": "^1.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-firefox-launcher": "^1.1.0",
    "karma-istanbul": "0.0.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^3.0.5",
    "mocha": "^6.0.2",
    "sinon": "^7.0.0",
    "sinon-chai": "^3.2.0",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "yargs": "^13.2.2"
  },
  "dependencies": {
    "vanillajs-helpers": "^2.0.1"
  }
}
